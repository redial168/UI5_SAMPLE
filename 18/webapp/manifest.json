{
	"_version": "1.12.0",
	"sap.app": {
	  "id": "sap.ui5.walkthrough",
	  "type": "application",
	  "i18n": "i18n/i18n.properties",
	  "title": "{{appTitle}}",
	  "description": "{{appDescription}}",
	  "applicationVersion": {
		"version": "1.0.0"
	  }
	},
	"sap.ui": {
	  "technology": "UI5",
	  "deviceTypes": {
		  "desktop": true,
		  "tablet": true,
		  "phone": true
	  }
	},
	"sap.ui5": {
	  "rootView": {
		  "viewName": "sap.ui5.walkthrough.view.App",
		  "type": "XML",
		  "id": "app"
	  },
	  "dependencies": {
		"minUI5Version": "1.93",
		"libs": {
		  "sap.ui.core": {},
		  "sap.m": {}
		}
	  },
	  "models": {
		"invoice": {
			"type": "sap.ui.model.json.JSONModel",
			"uri": "Invoices.json"
		}
	  },
	  "resources": {
		"css": [
		  {
			"uri": "css/style.css"
		  }
		]
	  }
	}
  }