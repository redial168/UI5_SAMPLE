<mvc:View
	controllerName="sap.ui5.walkthrough.controller.App"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<Input 
	    id="myinput"
		value="{/name}"
		width="60%"/>

	<Button
		text="Model->Control"
		press=".changeModel"/>
	<Button
		text="Control->Model"
		press=".changeControl"/>
</mvc:View>
